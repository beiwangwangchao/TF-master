            <xml-property name="queryText"><![CDATA[SELECT
	ist.TRX_ID,
	ist.TRX_NUMBER,
	ist.ORGANIZATION_ID,
	(
		SELECT
			tl. NAME
		FROM
			SPM_INV_ORGANIZATION_B b
		LEFT OUTER JOIN SPM_INV_ORGANIZATION_TL tl ON (
			b.INV_ORG_ID = tl.INV_ORG_ID
			AND tl.lang = ?
		)
		WHERE
			b.INV_ORG_ID = ist.ORGANIZATION_ID
	) AS INV_ORG_NAME,
	ist.TRX_DATE,
	(
		SELECT
			tl.MEANING
		FROM
			SYS_CODE_VALUE_B b
		LEFT OUTER JOIN SYS_CODE_VALUE_TL tl ON (
			b.CODE_VALUE_ID = tl.CODE_VALUE_ID
			AND tl.lang = ?
		)
		WHERE
			b.
		VALUE
			= ist.TRX_TYPE
		AND b.CODE_ID = (
			SELECT
				CODE_ID
			FROM
				SYS_CODE_B scb
			WHERE
				scb.CODE = 'INV.ADJUSTMENT_TYPE'
		)
	) AS TRX_TYPE,
	ist.VENDOR_ID,
	(
		SELECT
			tl. NAME
		FROM
			po_vendor_b b
		LEFT OUTER JOIN po_vendor_tl tl ON (
			b.vendor_id = tl.vendor_id
			AND tl.lang = ?
		)
		WHERE
			b.vendor_id = ist.vendor_id
	) AS VENDOR_NAME,
	--ist.STATUS,
	(
		SELECT
			tl.MEANING
		FROM
			SYS_CODE_VALUE_B b
		LEFT OUTER JOIN SYS_CODE_VALUE_TL tl ON (
			b.CODE_VALUE_ID = tl.CODE_VALUE_ID
			AND tl.lang = ?
		)
		WHERE
			b.
		VALUE
			= ist.STATUS
		AND b.CODE_ID = (
			SELECT
				CODE_ID
			FROM
				SYS_CODE_B scb
			WHERE
				scb.CODE = 'INV.STOCK_IN_OUT_STATUS'
		)
	) AS STATUS,
	--istd.OPER_REASON_CODE,
	(
		SELECT
			tl.MEANING
		FROM
			SYS_CODE_VALUE_B b
		LEFT OUTER JOIN SYS_CODE_VALUE_TL tl ON (
			b.CODE_VALUE_ID = tl.CODE_VALUE_ID
			AND tl.lang = ?
		)
		WHERE
			b.
		VALUE
			= istd.OPER_REASON_CODE
		AND b.CODE_ID = (
			SELECT
				CODE_ID
			FROM
				SYS_CODE_B scb
			WHERE
				scb.CODE = 'INV.STOCK_REASON'
		)
	) AS OPER_REASON_CODE,
	ist.REMARK
FROM
	INV_STOCK_TRX ist,
	INV_STOCK_TRX_DETAIL istd
WHERE
	IST.TRX_ID = ISTD.TRX_ID
AND IST.TRX_ID = ?]]></xml-property>
