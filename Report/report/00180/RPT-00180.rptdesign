                    <property name="name">EXPIRY_DATE</property>
                    <property name="nativeName">EXPIRY_DATE</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">PACKING_TYPE</property>
                    <property name="nativeName">PACKING_TYPE</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">UNIT_OF_CARTON</property>
                    <property name="nativeName">UNIT_OF_CARTON</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">9</property>
                    <property name="name">QTY</property>
                    <property name="nativeName">QTY</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">10</property>
                    <property name="name">QUANTITY</property>
                    <property name="nativeName">QUANTITY</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">11</property>
                    <property name="name">REMARK</property>
                    <property name="nativeName">REMARK</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT
	istd.ITEM_ID,
	iib.ITEM_NUMBER,
	(
		SELECT
			tl.ITEM_NAME
		FROM
			INV_ITEM_B b
		LEFT OUTER JOIN INV_ITEM_TL tl ON (
			b.ITEM_ID = tl.ITEM_ID
			AND tl.lang = ?
		)
		WHERE
			tl.ITEM_ID = istd.ITEM_ID
	) AS ITEM_NAME,
	iuomtl.NAME UOM_NAME,
	istd.LOT_NUMBER,
	(
		CASE
		WHEN istd.EXPIRY_DATE IS NULL THEN
			''
		ELSE
			"TO_CHAR"(istd.EXPIRY_DATE, 'yyyy-MM-dd')
		END
	) EXPIRY_DATE,
	(
		SELECT
			tl. NAME
		FROM
			inv_unit_of_measure_b b
		LEFT OUTER JOIN inv_unit_of_measure_tl tl ON (
			b.UOM_CODE = tl.UOM_CODE
			AND tl.lang = ?
		)
		WHERE
			tl.UOM_CODE = istd.PACKING_TYPE
	) AS PACKING_TYPE,
	istd.UNIT_OF_CARTON,
	(
		CASE
		WHEN istd.UNIT_OF_CARTON = 0
		OR istd.UNIT_OF_CARTON = '' THEN
			istd.QUANTITY
		ELSE
			istd.QUANTITY / istd.UNIT_OF_CARTON
		END
	) Qty,
	istd.QUANTITY,
	istd.REMARK
FROM
	INV_STOCK_TRX_DETAIL istd, INV_ITEM_B iib, inv_unit_of_measure_tl iuomtl
WHERE
	istd.ITEM_ID = iib.ITEM_ID
	AND iuomtl.UOM_CODE = iib.UOM_CODE
	AND iuomtl.lang = ?
	AND TRX_ID = ?
ORDER BY istd.rowid ASC]]></xml-property>
        </oda-data-set>
    </data-sets>
