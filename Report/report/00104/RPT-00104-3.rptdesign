            <method name="beforeOpen"><![CDATA[this.queryText = this.queryText.replace("listparams",params["invOrgIdList"].value);
if(params["adjStatus"].value !=null &amp;&amp; params["adjStatus"].value !=""){
  this.queryText = this.queryText.replace("stStatus","ist.status IN (" +"\'"+params["adjStatus"].value.replace(/,/g,"\',\'")+"\'"+")");
