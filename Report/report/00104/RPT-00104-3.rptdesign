            <xml-property name="queryText"><![CDATA[SELECT to_char(ist.trx_date, 'yyyy/MM/dd') trx_date,
       (select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = ist.trx_type
           and scb.code = 'INV.ADJUSTMENT_TYPE'
           and scb.code_id = scvb.code_id) trx_type,
       (select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = ist.status
           and scb.code = 'INV.STOCK_IN_OUT_STATUS'
           and scb.code_id = scvb.code_id) trx_status,
       iib.item_number,
       iib.item_name,
       istd.lot_number,
       to_char(istd.expiry_date, 'yyyy/MM/dd') expiry_date,
       SUM(istd.quantity) quantity
  FROM inv_stock_trx ist, inv_stock_trx_detail istd, inv_item_b iib
 WHERE ist.trx_id = istd.trx_id
   AND istd.item_id = iib.item_id
   AND ist.organization_id IN (listparams)
   AND stTrxType
   AND (ist.trx_date >= to_date(?, 'yyyy/mm/dd') OR ? IS NULL )
   AND (ist.trx_date < (to_date(?, 'yyyy/mm/dd') + 1) OR ? IS NULL )
   AND stStatus
   AND (iib.item_number >= ? OR ? IS NULL )
   AND (iib.item_number <= ? OR ? IS NULL )
 GROUP BY ist.trx_date, iib.item_number,ist.trx_type,ist.status,iib.item_name, istd.lot_number, istd.expiry_date]]></xml-property>
