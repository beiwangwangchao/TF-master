            <xml-property name="queryText"><![CDATA[select (select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = st.trx_type
           and scb.code = 'INV.ADJUSTMENT_TYPE'
           and scb.code_id = scvb.code_id) trx_type,
       (select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = st.status
           and scb.code = 'INV.STOCK_IN_OUT_STATUS'
           and scb.code_id = scvb.code_id) trx_status,
       iib.item_number,
       coalesce(iibct.item_name, iib.item_name) item_name,
       std.lot_number,
       to_char(std.expiry_date, 'yyyy/MM/dd') expiry_date,
       sum(std.quantity) qty
  from inv_stock_trx st, inv_stock_trx_detail std, inv_item_b iib
  left outer join inv_item_tl iibct
    on (iib.item_id = iibct.item_id and iibct.lang = ?)
 where st.trx_id = std.trx_id
   and std.item_id = iib.item_id
   and to_char(st.trx_date, 'yyyy/MM/dd') >= (case when coalesce(?, '*') = '*' then to_char(st.trx_date, 'yyyy/MM/dd') else ? end)
   and to_char(st.trx_date, 'yyyy/MM/dd') <= (case when coalesce(?, '*') = '*' then to_char(st.trx_date, 'yyyy/MM/dd') else ? end)
   --and st.status = (case when coalesce(?, '*') = '*' then st.status else ? end)
   --and st.trx_type = (case when coalesce(?, '*') = '*' then st.trx_type else ? end)
   and stStatus
   and stTrxType
   and iib.item_id >= (case when coalesce(?, '*') = '*' 
   							then iib.item_id 
   							else (select item_id from inv_item_b where item_number = ?)
   					   end)
   and iib.item_id >= (case when coalesce(?, '*') = '*' 
   							then iib.item_id 
   							else (select item_id from inv_item_b where item_number = ?)
   					   end)
   and st.organization_id in (orgIdParamsList)
 group by st.trx_type,
          iib.item_number,
          st.status,
          iibct.item_name,
          iib.item_name,
          std.lot_number,
          std.expiry_date]]></xml-property>
