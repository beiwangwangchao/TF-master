                </list-property>
            </structure>
            <method name="beforeOpen"><![CDATA[if(params["adjType"].value !=null &amp;&amp; params["adjType"].value !=""){
  this.queryText = this.queryText.replace("adjtypes","scvb.value IN (" +"\'"+params["adjType"].value.replace(/,/g,"\',\'")+"\'"+")");
} else {
  this.queryText = this.queryText.replace("adjtypes","1=1");
}]]></method>
            <property name="dataSource">Data Source</property>
            <list-property name="resultSet">
                <structure>
                    <property name="position">1</property>
                    <property name="name">TYPE_MEANING</property>
                    <property name="nativeName">TYPE_MEANING</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[select
	listagg (type_meaning, ', ') within group (order by type_meaning) as type_meaning
from
	(select scb.code type_code, scvb.value type_value, coalesce(scvt.meaning, scvb.meaning) type_meaning
  from sys_code_b scb, sys_code_value_b scvb
  left outer join sys_code_value_tl scvt
    ON (scvb.code_value_id = scvt.code_value_id AND
       scvt.lang = ?)
 where adjtypes
 and scb.code = 'INV.ADJUSTMENT_TYPE'
 and scb.code_id = scvb.code_id	)]]></xml-property>
        </oda-data-set>
