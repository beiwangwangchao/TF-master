                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">QUANTITY</property>
                    <property name="nativeName">QUANTITY</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">UNIT_COST</property>
                    <property name="nativeName">UNIT_COST</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">TOTALCOST</property>
                    <property name="nativeName">TOTALCOST</property>
                    <property name="dataType">decimal</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[
SELECT COST_ORG_ID,
       CATEGORY,
       ITEM_NUMBER,
       ITEM_NAME,
       QUANTITY,
       UNIT_COST,
       ROUND((QUANTITY * UNIT_COST), 0) TOTALCOST
  FROM (SELECT TW_UNIT_COST.COST_ORG_ID,
               TW_UNIT_COST.CATEGORY,
               TW_UNIT_COST.ITEM_NUMBER,
               TW_UNIT_COST.ITEM_NAME,
               SUM(TW_UNIT_COST.QUANTITY) QUANTITY,
               TW_UNIT_COST.UNIT_COST
          FROM (SELECT ITEM_UNIT_COST.COST_ORG_ID,
                       IIB.ITEM_NUMBER,
                       IITL.ITEM_NAME,
                       TO_CHAR(IST.TRX_DATE, 'YYYY/MM') TRX_DATE,
                       (SELECT SCVTL.MEANING
                          FROM SYS_CODE_B SCB, SYS_CODE_VALUE_B SCVB
                          LEFT OUTER JOIN SYS_CODE_VALUE_TL SCVTL
                            ON SCVB.CODE_VALUE_ID = SCVTL.CODE_VALUE_ID
                           AND SCVTL.LANG = ?
                         WHERE SCB.CODE_ID = SCVB.CODE_ID
                           AND SCB.CODE = 'INV.STOCK_REASON'
                           AND SCVB.VALUE = ISTD.OPER_REASON_CODE) CATEGORY,
                       ISTD.QUANTITY,
                       ITEM_UNIT_COST.UNIT_COST
                  FROM INV_STOCK_TRX IST
                  LEFT OUTER JOIN INV_STOCK_TRX_DETAIL ISTD
                    ON IST.TRX_ID = ISTD.TRX_ID
                  LEFT OUTER JOIN INV_ITEM_B IIB
                    ON ISTD.ITEM_ID = IIB.ITEM_ID
                  LEFT OUTER JOIN INV_ITEM_TL IITL
                    ON ISTD.ITEM_ID = IITL.ITEM_ID
                   AND IITL.LANG = ?
                  LEFT OUTER JOIN (SELECT SUM(ICR.UNIT_COST) UNIT_COST,
                                         ICR.COST_ORG_ID,
                                         ICR.ITEM_ID,
                                         ICR.YEAR,
                                         ICR.MONTH
                                    FROM INV_COST_RECORDS ICR
                                   WHERE ICR.STATUS = 'P'
                                   GROUP BY (ICR.COST_ORG_ID, ICR.ITEM_ID,
                                             ICR.YEAR, ICR.MONTH)) ITEM_UNIT_COST
                    ON ITEM_UNIT_COST.ITEM_ID = ISTD.ITEM_ID
                   AND ITEM_UNIT_COST.YEAR = TO_CHAR(IST.TRX_DATE, 'YYYY')
                   AND ITEM_UNIT_COST.MONTH = TO_CHAR(IST.TRX_DATE, 'MM')
                 WHERE IST.TRX_TYPE = 'STKOT'
                   AND IST.STATUS = 'COMP'
				   -- AND ITEM_UNIT_COST.COST_ORG_ID = 1
                   AND IST.ORGANIZATION_ID IN
                       (SELECT SODB.INV_ORG_ID
                          FROM SPM_ORG_DEFINATION_B SODB
                         WHERE SODB.COST_ORG_ID = ?)
                   AND TO_CHAR(IST.TRX_DATE, 'YYYY/MM/DD') >= ?
                   AND TO_CHAR(IST.TRX_DATE, 'YYYY/MM/DD') <= ?
                   AND ISTD.ITEM_ID >=
                       COALESCE(TO_NUMBER(?), ISTD.ITEM_ID)
                   AND ISTD.ITEM_ID <=
                       COALESCE(TO_NUMBER(?), ISTD.ITEM_ID)) TW_UNIT_COST
         GROUP BY TW_UNIT_COST.COST_ORG_ID,
                  TW_UNIT_COST.CATEGORY,
                  TW_UNIT_COST.ITEM_NUMBER,
                  TW_UNIT_COST.TRX_DATE,
                  TW_UNIT_COST.ITEM_NAME,
                  TW_UNIT_COST.UNIT_COST) TOTAL_UNIT_COST
]]></xml-property>
        </oda-data-set>
