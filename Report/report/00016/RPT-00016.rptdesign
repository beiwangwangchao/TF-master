                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">PV</property>
                    <property name="nativeName">PV</property>
                    <property name="dataType">decimal</property>
                </structure>
                <structure>
                    <property name="position">9</property>
                    <property name="name">RANK</property>
                    <property name="nativeName">RANK</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT
	OSO.ORDER_NUMBER,
	OSO.INVOICE_NUMBER,
	(
		CASE
		WHEN EXISTS (
			SELECT
				OSR.RETURN_ID
			FROM
				OM_SALES_RETURN osr
			WHERE
				OSR.ORDER_HEADER_ID = OSO.HEADER_ID
			AND OSR.RETURN_STATUS <> 'NEW'
		) THEN
			'*'
		ELSE
			''
		END
	) AS RETURN,
	OSO.PAY_DATE AS ORDER_DATE,
	(
		SELECT
			SCVT.MEANING
		FROM
			SYS_CODE_B SYCB,
			SYS_CODE_VALUE_B SCVB,
			SYS_CODE_VALUE_TL SCVT
		WHERE
			SYCB.CODE = 'OM.SALES_STATUS'
		AND SYCB.CODE_ID = SCVB.CODE_ID
		AND SCVB."VALUE" = OSO.ORDER_STATUS
		AND SCVB.CODE_VALUE_ID = SCVT.CODE_VALUE_ID
		AND SCVT.LANG = ?
	) AS ORDER_STATUS,
	MM.MEMBER_CODE || ' ' || CASE
WHEN MM.ENGLISH_FIRST_NAME || ' ' || MM.ENGLISH_LAST_NAME = ' ' THEN
	MM.CHINESE_FIRST_NAME || ' ' || MM.CHINESE_LAST_NAME
ELSE
	MM.ENGLISH_FIRST_NAME || ' ' || MM.ENGLISH_LAST_NAME
END AS MEMBER_ID,
	OSO.ACTRUAL_PAY_AMT AS ORDER_AMT,
	(
		SELECT
			SUM (OSL.PV*OSL.QUANTITY)
		FROM
			OM_SALES_LINE OSL
		WHERE
			OSL.HEADER_ID = OSO.HEADER_ID
	) AS PV,
 (
	SELECT
		SCVT.MEANING
	FROM
		SYS_CODE_B SYCB,
		SYS_CODE_VALUE_B SCVB,
		SYS_CODE_VALUE_TL SCVT,
		OMK.OMK_RTL_SALARY_BALANCE ORSB
	WHERE
		SYCB.CODE = 'MM.MEMBER_RANK'
	AND SYCB.CODE_ID = SCVB.CODE_ID
	AND SCVB."VALUE" = ORSB.DEALER_POST_CODE
	AND SCVB.CODE_VALUE_ID = SCVT.CODE_VALUE_ID
	AND SCVT.LANG = ?
	AND ORSB.DEALER_NO = MM.MEMBER_CODE
) AS RANK
FROM
	OM_SALES_ORDER OSO
LEFT OUTER JOIN MM_MEMBER MM ON (
	MM.MEMBER_ID = OSO.MEMBER_ID
),
 SPM_SALES_ORGANIZATION_B SSOB
WHERE
	SSOB.SALES_ORG_ID IN (listparms)
AND OSO.SALES_ORG_ID = SSOB.SALES_ORG_ID
AND (OSO.PAY_DATE >= TO_DATE (?, 'yyyy/mm/dd'))
AND (OSO.PAY_DATE < NVL ((TO_DATE(?, 'yyyy/MM/dd') + 1),SYSDATE))
AND (
	? IS NULL
	OR (
		MM.MEMBER_CODE = ?
	)
)
AND (
	? IS NULL
	OR OSO.ORDER_NUMBER >= ?
)

AND (
	? IS NULL
	OR OSO.ORDER_NUMBER <= ?
)
--AND (
--	'listcate1' IS NULL
--	OR ICB.CATEGORY_ID IN (listcate2)
--)
AND (
	'listorderstatus1' IS NULL 
	OR OSO.ORDER_STATUS IN (listorderstatus2)
)
ORDER BY OSO.ORDER_DATE, OSO.ORDER_NUMBER]]></xml-property>
