            <method name="beforeOpen"><![CDATA[var str = this.queryText;
str = str.replace("listparms", params["SalesOrgIdList"]);
if(params["OrderStatusList"].value){
	str = str.replace("listorderstatus1", params["OrderStatusList"]);
	str = str.replace("listorderstatus2", ("\'"+params["OrderStatusList"].value.replace(/,/g,"\',\'")+"\'"));
}else{
	str = str.replace("listorderstatus1", "");
	str = str.replace("listorderstatus2", "\'\'");
}
this.queryText = str;]]></method>
            <property name="dataSource">Data Source</property>
