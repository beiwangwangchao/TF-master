            <xml-property name="queryText"><![CDATA[SELECT
	listagg (CATEGORY_NAME, ',') WITHIN GROUP (ORDER BY CATEGORY_NAME) AS CATEGORY_NAME
FROM
	(
		SELECT
			ICT.CATEGORY_NAME
		FROM
			INV_CATEGORY_B ICB,
			INV_CATEGORY_TL ICT
		WHERE
			ICT.CATEGORY_ID = ICB.CATEGORY_ID
		AND ICT.LANG = ?
		AND ICB.PARENT_CATEGORY_ID = 0
	)]]></xml-property>
