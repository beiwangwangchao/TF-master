          from spm_sales_organization_b sso,
               spm_market_b             sm,
               spm_company_tl           sc
         where sso.market_id = sm.market_id
           and sm.company_id = sc.company_id
           and sc.lang = ?
           and sso.sales_org_id = ?]]></xml-property>
