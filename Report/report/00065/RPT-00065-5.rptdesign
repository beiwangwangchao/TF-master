            <xml-property name="queryText"><![CDATA[select distinct mkt.mkt_code,
       mkt.sorg_name,
       to_char(SO.pay_date, 'yyyy/mm/dd') as order_date,
       so.order_number,
       so.invoice_number, 
       mem.member_code,
	   so.delivery_type,
       nvl(sopvamt.order_total_pv, 0) as order_total_PV,
       nvl(sopvamt.order_total_bftax_amt, 0) as Prod_total_bftax_amt2,
       cast((nvl(soship.shipping_fee, 0) / 1.06) as decimal(10, 1)) as shipping_fee_BFTax2,
       (nvl(sopvamt.order_total_bftax_amt, 0) +
       cast(((nvl(soship.shipping_fee, 0) / 1.06)) as decimal(10, 1))) as Order_bftax_amt2,
       nvl(so.tax_amt, 0) +
       cast((nvl(soship.shipping_fee, 0) * 0.06) as decimal(10, 1)) as total_tax_amt2,
       nvl(so.discount_amt, 0) as discount_amt2,
       nvl((select sum(osa.adjustment_amount)
                     from OM_SALES_ADJUSTMENT osa
                    where osa.header_id = so.header_id),
                   0) as adjustment_amount2, 
       /*(nvl(sopvamt.order_total_bftax_amt, 0) +
       cast((nvl(soship.shipping_fee, 0) / 1.06) as decimal(10, 1)) +
       nvl(so.tax_amt, 0) +
       cast((nvl(soship.shipping_fee, 0) * 0.06) as decimal(10, 1)) -
       nvl(so.discount_amt, 0) + nvl(soadj.adjustment_amount, 0)) as Cal_Pay_Amt2,*/
       nvl(so.actrual_pay_amt, 0) as actrual_pay_amt2,
       soadj.remark as Adj_Remark,
       nvl2(soadj.adjustment_amount, 'Y', 'N') as with_adjustment

  from OM_Sales_order SO
  left join mm_member mem
    on mem.member_id = so.member_id
  left join (select so.header_id,
                    so.order_number,
                    sum(sol. PV * sol.quantity) as order_total_PV,
                    sum(sol.unit_selling_price * sol.quantity) as order_total_Amt,
                    sum(sol.unit_orig_price * sol.quantity) as order_total_BFTax_Amt
               from om_sales_order so, om_sales_line sol
              where so.header_id = sol.header_id
                and so.order_status in (OrderStatusList)
                and to_char(SO.pay_date, 'yyyy-mm-dd') >= ?
                and to_char(SO.pay_date, 'yyyy-mm-dd') <= coalesce(?,to_char(sysdate, 'yyyy-mm-dd'))
              group by so.order_number, so.header_id) SOPVAMT
    on so.header_id = sopvamt.header_id
  left join om_sales_logistics soship
    on so.header_id = soship.header_id
  left join om_sales_adjustment soadj
    on so.header_id = soadj.header_id
  left join om_sales_voucher sovch
    on so.header_id = sovch.header_id
  left join (select mk.code           as mkt_code,
                    mk.market_id,
                    sorg.sales_org_id,
                    sorg.code         as sorg_code,
                    tl.lang,
                    tl.name           as sorg_name,
                    sorg.location_id  as sorg_location_id,
                    sorg.enabled_flag as sorg_enabled_flag 
               from spm_sales_organization_b sorg
               left join spm_sales_organization_TL TL
                 on sorg.sales_org_id = tl.sales_org_id
                and tl.lang = ?
               left join spm_market_b mk
                 on sorg.market_id = mk.market_id) mkt
    on so.sales_org_id = mkt.sales_org_id
    
 where mkt.sales_org_id in (SalesOrgIdList)
   and to_char(SO.pay_date, 'yyyy-mm-dd') >= ?
   and to_char(SO.pay_date, 'yyyy-mm-dd') <= coalesce(?,to_char(sysdate, 'yyyy-mm-dd'))
   and so.order_status in (OrderStatusList)
   and so.order_type in (OrderTypeList)
   --and soadj.adjustment_amount <> 0

 order by mkt.mkt_code, mkt.sorg_name, so.order_number
]]></xml-property>
