            <xml-property name="queryText"><![CDATA[SELECT scv.meaning member_role,
       mm.member_code,
       decode(mm.chinese_last_name || mm.chinese_first_name,
              NULL,
              mm.english_last_name || mm.english_first_name,
              mm.chinese_last_name || mm.chinese_first_name) member_name,
       oso.order_number,
       to_char(oso.pay_date, 'yyyy/MM/dd') pay_date,
       iib.item_number,
       iib.item_name,
       SUM(coalesce(osl.quantity, 0)) qty,
       to_char(SUM(coalesce(osl.quantity * osl.unit_selling_price, 0)), 'FM999,999,999,999,990.00') sales_amt,
       to_char(SUM(coalesce(osl.quantity * osl.pv, 0)), 'FM999,999,999,999,990.00') sales_pv
  FROM om_sales_order    oso,
       mm_member         mm,
       om_sales_line     osl,
       inv_item_b        iib,
       sys_code_value_vl scv
 WHERE oso.member_id = mm.member_id
   AND oso.header_id = osl.header_id
   AND oso.sales_org_id = osl.sales_org_id
   AND iib.item_id = osl.item_id
   AND mm.termination_date IS NULL
   AND to_char(oso.pay_date, 'YYYY/MM/DD') BETWEEN ? AND ?
   AND oso.sales_org_id IN (orglistparams)
   AND itemlistparams
   AND oso.order_status IN (statuslistparams)
   AND rolelistparams
   AND memberlistparams
   AND periodlistparams
   AND scv.code = 'MM.MEMBER_ROLE'
   AND scv.value = mm.member_role
   AND scv.lang = ?
 GROUP BY oso.header_id,
          scv.meaning,
          mm.member_code,
          decode(mm.chinese_last_name || mm.chinese_first_name,
                 NULL,
                 mm.english_last_name || mm.english_first_name,
                 mm.chinese_last_name || mm.chinese_first_name),
          oso.order_number,
          to_char(oso.pay_date, 'yyyy/MM/dd'),
          iib.item_number,
          iib.item_name
 ORDER BY oso.header_id,
          scv.meaning,
          mm.member_code,
          decode(mm.chinese_last_name || mm.chinese_first_name,
                 NULL,
                 mm.english_last_name || mm.english_first_name,
                 mm.chinese_last_name || mm.chinese_first_name),
          oso.order_number,
          to_char(oso.pay_date, 'yyyy/MM/dd'),
          iib.item_number,
          iib.item_name]]></xml-property>
        </oda-data-set>
