            <xml-property name="queryText"><![CDATA[SELECT
concat(IIB.ITEM_NUMBER, concat(' ',IIB.ITEM_NAME)), --商品编号+商品名称
OSO.ORDER_NUMBER, --订单编号
OSO.INVOICE_NUMBER, --发票编号
to_char(OSO.ORDER_DATE, 'yyyy/MM/dd') ord_date, --订单日期
(select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = OSO.ORDER_TYPE
           and scb.code = 'OM.ORDER_TYPE'
           and scb.code_id = scvb.code_id) Sales_Order_Type, --订单类型
(select coalesce(scvt.meaning, scvb.meaning)
          from sys_code_b scb, sys_code_value_b scvb
          left outer join sys_code_value_tl scvt
            ON (scvb.code_value_id = scvt.code_value_id AND
               scvt.lang = ?)
         where scvb.value = OSL.ITEM_SALES_TYPE
           and scb.code = 'OM.PRODUCT_SALES_TYPE'
           and scb.code_id = scvb.code_id) ITEM_SALES_TYPE, --销售类型
coalesce(OSL.QUANTITY,0), -- 数量
coalesce(OSRL.QUANTITY,0), --退货数量
OSO.REMARK, --备注
SSO.CODE,
IIB.ITEM_NUMBER
FROM 
OM_SALES_ORDER OSO
	left outer join OM_SALES_RETURN OSR on
	(osr.order_header_id =oso.header_id
	AND OSR.RETURN_STATUS != 'NEW'),
SPM_SALES_ORGANIZATION_B SSO,
OM_SALES_LINE OSL
	LEFT OUTER JOIN INV_ITEM_B iib on 
    (OSL.ITEM_ID = IIB.ITEM_ID)
	LEFT OUTER JOIN OM_SALES_RTN_LINE osrl on 
    (osl.LINE_ID = osrl.ORDER_LINE_ID)
WHERE
    OSO.SALES_ORG_ID IN (listparms)
	AND OSO.HEADER_ID = OSL.HEADER_ID
    AND OSO.SALES_ORG_ID = SSO.SALES_ORG_ID
    AND (
		'listordertype1' IS NULL 
		OR OSO.ORDER_TYPE IN (listordertype2)
	)
    AND (
		'listorderstatus1' IS NULL 
		OR OSO.ORDER_STATUS IN (listorderstatus2)
	)
	AND  (oso.order_date >= to_date(?, 'yyyy/MM/dd') OR ? IS NULL )
	AND (oso.order_date < (to_date(?, 'yyyy/MM/dd') + 1) OR ? IS NULL )
	AND (OSL.ITEM_SALES_TYPE IS NULL OR OSL.ITEM_SALES_TYPE = ? OR ? IS NULL)
	and (iib.item_number >= ? or ? is null)
	and (iib.item_number <= ? or ? is null)]]></xml-property>
