            <method name="beforeOpen"><![CDATA[this.queryText = this.queryText.replace("typelist", params["orderTypeList"]);
var str = this.queryText;
//销售组织
str = str.replace("listparms", params["SalesOrgIdList"]);
//2.替换订单状态参数
if(params["orderStatusList"].value){
	str = str.replace("listorderstatus1", params["orderStatusList"]);
	str = str.replace("listorderstatus2", ("\'"+params["orderStatusList"].value.replace(/,/g,"\',\'")+"\'"));
}else{
	str = str.replace("listorderstatus1", "");
	str = str.replace("listorderstatus2", "\'\'");
}
//订单类型
if(params["orderTypeList"].value){
	str = str.replace("listordertype1", params["orderTypeList"]);
	str = str.replace("listordertype2", ("\'"+params["orderTypeList"].value.replace(/,/g,"\',\'")+"\'"));
}else{
	str = str.replace("listordertype1", "");
	str = str.replace("listordertype2", "\'\'");
}
this.queryText = str;
]]></method>
