            <xml-property name="queryText"><![CDATA[
select iob.code invOrgCode,
       ib.item_number productCode,
       it.item_name productName,
       replace(icr.year, ',', '') year,
       icr.month,
       icr.quantity,
       icr.lot_number,
       to_char(icr.expiry_date, 'yyyy/MM/dd') expiryDate,
       icr.unit_cost,
       icr.balance
  from inv_cost_records icr
  left outer join spm_inv_organization_b iob
    on iob.inv_org_id = icr.inv_org_id
  left outer join inv_item_b ib
    on ib.item_id = icr.item_id
  left outer join inv_item_tl it
    on it.item_id = icr.item_id
   and it.lang = ?
 where icr.inv_org_id in (invlist)
   and icr.status = 'P'
   and yearlist
   and monthlist
   and (ib.item_number >= ? or ? is null)
   and (ib.item_number <= ? or ? is null)
 group by icr.inv_org_id,
          iob.code,
          ib.item_number,
          it.item_name,
          icr.year,
          icr.month,
          icr.quantity,
          icr.lot_number,
          icr.expiry_date,
          icr.unit_cost,
          icr.unit_cost,
          icr.balance
 order by icr.inv_org_id, icr.year desc, icr.month desc, ib.item_number
]]></xml-property>
